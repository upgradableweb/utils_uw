{
  "name": "easyfetch_uw",
  "version": "1.0.0",
  "description": "A simple utility to fetchit data from url end point.",
  "repository": {
    "type": "git",
    "url": "https://github.com/upgradableweb/utils_uw/tree/main/packages/easyfetch_uw"
  },
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./params": {
      "types": "./dist/params.d.ts",
      "import": "./dist/params.js",
      "require": "./dist/params.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "publish": "npm publish --access=public"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "jest-fetch-mock"
    ]
  },
  "keywords": [
    "text",
    "join",
    "utility"
  ],
  "author": "Barath",
  "license": "MIT",
  "devDependencies": {
    "jest": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "tslib": "^2.8.1",
    "typescript": "^5.8.2",
    "vite": "^6.2.0",
    "vite-plugin-dts": "^4.5.3"
  }
}